{
  "name": "@akashic/akashic-cli-install",
  "version": "0.3.2",
  "description": "npm install wrapper module for Akashic games",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "gulp clean && npm run install:typings && gulp compile",
    "install:typings": "gulp install:typings",
    "clean:typings": "gulp clean:typings",
    "clean": "gulp clean",
    "start": "npm run build && node bin/run",
    "build": "gulp compile",
    "lint": "gulp lint lint-md",
    "test": "gulp test && gulp lint && gulp lint-md"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-install": "./bin/run"
  },
  "files": [
    "bin",
    "lib",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "del": "~3.0.0",
    "gulp": "~4.0.0",
    "gulp-istanbul": "~1.1.3",
    "gulp-jasmine": "~2.3.0",
    "gulp-shell": "~0.5.2",
    "gulp-tslint": "~8.1.3",
    "jasmine": "~3.1.0",
    "jasmine-reporters": "~2.0.4",
    "jasmine-terminal-reporter": "~0.9.1",
    "mdast": "~2.3.2",
    "mdast-lint": "~1.1.1",
    "mock-fs": "~4.4.2",
    "tslint": "~5.11.0",
    "typescript": "2.1.6",
    "typings": "^1.0.4"
  },
  "dependencies": {
    "@akashic/akashic-cli-commons": "~0.2.9",
    "commander": "2.8.1"
  },
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/akashic-games/akashic-cli-install.git"
  },
  "publishConfig": {
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
